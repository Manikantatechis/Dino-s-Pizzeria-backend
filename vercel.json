{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings"
  },
  "functions": {
    "api": {
      "handler": "config.wsgi.application",
      "memory": 1024,
      "maxDuration": 10,
      "timeout": 10,
      "runtime": "vercel-python@0.5.1",
      "includeFiles": ["config/**/*"]
    }
  }
}
